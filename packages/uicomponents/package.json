{
  "name": "@dhavalptl/uicomponents",
  "author": "Dhaval",
  "version": "1.0.12",
  "description": "React UI Components Starter",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/cjs/index.d.ts",
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "ui-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dhavalptl/monorepo-app.git",
    "directory": "packages/uicomponents"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clear": "rm -rf ./lib",
    "start": "tsc --watch",
    "build": "npm run clear && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "test": "jest",
    "lint": "eslint src --ext .tsx,.ts,.js",
    "lint:fix": "eslint src --ext .tsx,.ts,.js --fix",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 5000"
  },
  "peerDependencies": {
    "react": ">=16.13.1"
  },
  "devDependencies": {
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "gitHead": "6e99b1c0a6b7d40933c27ced9830387139b9b3fd"
}
